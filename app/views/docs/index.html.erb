<div class="row">
  <div  class="docs small-12 medium-6">
    <h3> <%= t('docs.index.title') %> </h3>
    <ul>
      <% Doc.to_show(current_user).each do |doc| %>
        <li>
          <p> <%= link_to doc.name, asset_path(doc.url), { target: link_doc_target(doc.format)}  %> </p>
          <p> <%= doc.description %></p>
        </li>
      <% end %>
    </ul>
  </div>
</div>