<div class="row">
  <div  class="docs small-12 medium-6">
    <h3> Manuales </h3>
    <ul>
      <% @docs.each do |doc| %>
        <li>
          <p> <%= link_to doc.name, asset_path(doc.url) , { target: '_blank' }  %> </p>
          <p> <%= doc.description %></p>
        </li>
      <% end %>
    </ul>
  </div>
</div>