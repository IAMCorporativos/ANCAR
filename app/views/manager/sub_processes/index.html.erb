<%= render partial: 'manager/shared/header',
           locals: {title: t("manager.sub_processes.#{action_name}.title"),
                    button: t("manager.sub_processes.index.create"),
                    path: new_manager_sub_process_path(period_id: params['period_id'],
                                                       organization_type_id: params['organization_type_id'],
                                                       main_process_id: params['main_process_id']),
                    modifiable: @main_process.modifiable?,
                    period: @main_process.period, main_process: @main_process, sub_process: nil, indicator: nil,
                    indicator_metric: nil} %>

<div>
  <% if @sub_processes.nil? || @sub_processes.empty? %>
    <strong> <%=t('manager.sub_processes.index.empty') %> </strong>
  <% else %>
    <%=  render 'item_list' %>
  <% end %>
</div>
